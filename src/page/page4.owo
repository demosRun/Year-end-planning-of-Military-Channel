    img.so-3.group-2(width="415" height="173" src="@|page3-3.png|")
    img.so-7.group-2(width="333" height="93" src="@|page3-7.png|")
    img.so-8.group-2(width="194" height="55" src="@|page3-8.png|")
    img.so-10.group-2(width="83" height="82" src="@|page3-10.png|")
