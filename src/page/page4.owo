    img.so-6.group-2(width="367" height="155" src="@|page3-6.png|")
    img.so-2(src="@|page3-2.png|")
    //- 上划提示
    img.upward(src="@|upward2.png|" o-animation="bottomToTopHide")
</template>

<script>
  module.exports = {
    created: function (params) {
      owo.tool.animate('slideInLeft', owo.query('.so-3')[0], 1000)
      owo.tool.animate('slideInRight', owo.query('.so-7')[0], 1600)
      owo.tool.animate('slideInRight', owo.query('.so-8')[0], 1600)
      // owo.tool.animate('slideInRight', owo.query('.so-10')[0], 1200)
      // owo.tool.animate('slideInLeft', owo.query('.so-9')[0], 1400)
      owo.tool.animate('slideInLeft', owo.query('.so-4')[0], 2600)
      owo.tool.animate('slideInRight', owo.query('.so-5')[0], 2600)
      owo.tool.animate('slideInLeft', owo.query('.so-6')[0], 3200)
      owo.tool.animate('zoomIn', owo.query('.so-10')[0], 4000)
      owo.tool.animate('zoomIn', owo.query('.so-9')[0], 4200)
      owo.tool.touch({
        el: this.$el,
        end: function (e) {
          if (e.swipe[1] < -100) {
            owo.go('page5', 'moveToTop', 'scaleUp', 'moveToBottom', 'scaleUp', true)
          } else if (e.swipe[1] > 100) {
            owo.go('page3', 'moveToBottom', 'scaleUp', 'moveToTop', 'scaleUp', true)
          }
        }
      })
    }
  }
</script>


